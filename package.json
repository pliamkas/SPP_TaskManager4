{
  "name": "task-manager-lab1",
  "version": "1.0.0",
  "description": "Simple task management application with server-side rendering",
  "main": "lab1.js",
  "scripts": {
    "start": "node lab1.js",
    "dev": "nodemon lab1.js",
    "start:lab2": "node lab2.js",
    "dev:lab2": "nodemon lab2.js",
    "server": "node server/lab2.server.js",
    "server:dev": "nodemon server/lab2.server.js",
    "client": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "dev:split": "concurrently \"npm:server:dev\" \"npm:client\"",
    "run": "concurrently \"npm:server:dev\" \"npm:client\"",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ejs": "^3.1.9",
    "multer": "^1.4.5-lts.1",
    "body-parser": "^1.20.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "task",
    "management",
    "express",
    "ejs",
    "server-side-rendering"
  ],
  "author": "",
  "license": "MIT"
}
